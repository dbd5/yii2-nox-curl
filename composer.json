{
  "name": "nox-it/yii2-nox-curl",
  "description": "Yii2 NOX cUrl",
  "type": "library",
  "keywords": [
    "yii2",
    "extension",
    "curl",
    "request",
    "framework",
    "php"
  ],
  "license": "BSD-3-Clause",
  "version": "1.0.5",
  "authors": [
    {
      "name": "Jonatas Sas",
      "email": "atendimento@jsas.com.br"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4.0",
    "nox-it/yii2-nox": "1.*",
