    "php-curl-class/php-curl-class": "7.*"
  },
  "autoload": {
    "psr-4": {
      "nox\\request\\": "request/"
    }
  },
  "config": {
    "process-timeout": 1800
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/jsas-development/yii2-nox-curl"
    }
