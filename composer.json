{
  "name": "nox-it/yii2-nox-curl",
  "description": "Yii2 NOX cUrl",
  "type": "library",
  "keywords": [
    "yii2",
    "extension",
    "curl",
    "request",
    "framework",
    "php"
  ],
  "license": "Apache-2.0",
  "version": "1.0.1",
  "authors": [
    {
      "name": "Jonatas Sas",
      "email": "atendimento@jsas.com.br"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "yiisoft/yii2": "2.*",
    "nox-it/yii2-nox": "1.*",
    "nox-it/yii2-nox-helpers": "1.*",
    "php-curl-class/php-curl-class": "4.*",
    "php": ">=5.4.0"
  },
  "autoload": {
    "psr-4": {
      "nox\\request\\": "request/"
    }
  },
  "config": {
    "process-timeout": 1800
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://bitbucket.org/nox-it-solutions/yii2-nox-curl"
    }
  ]
}
